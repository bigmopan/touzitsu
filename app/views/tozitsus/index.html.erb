<p id="notice"><%= notice %></p>

<h1>当日追加リスト</h1>

<table>
  <thead>
    <tr>
      <th>  納入先  </th>
      <th>  住所  </th>
      <th>  機種  </th>
      <th>  異常コード  </th>
      <th>  備考  </th>
      <th>  受付画面  </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tozitsus.each do |tozitsu| %>
      <tr>
        <td><%= tozitsu.name %></td>
        <td><%= tozitsu.address %></td>
        <td><%= tozitsu.kishu %></td>
        <td><%= tozitsu.error %></td>
        <td><%= tozitsu.content %></td>
        <td><%= image_tag tozitsu.image.url %></td>
        <td><%= link_to 'Show', tozitsu %></td>
        <td><%= link_to 'Edit', edit_tozitsu_path(tozitsu) %></td>
        <td><%= link_to 'Destroy', tozitsu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

