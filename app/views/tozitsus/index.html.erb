<p id="notice"><%= notice %></p>

<h1>Listing Tozitsus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Kishu</th>
      <th>Error</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tozitsus.each do |tozitsu| %>
      <tr>
        <td><%= tozitsu.name %></td>
        <td><%= tozitsu.address %></td>
        <td><%= tozitsu.kishu %></td>
        <td><%= tozitsu.error %></td>
        <td><%= tozitsu.content %></td>
        <td><%= link_to 'Show', tozitsu %></td>
        <td><%= link_to 'Edit', edit_tozitsu_path(tozitsu) %></td>
        <td><%= link_to 'Destroy', tozitsu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tozitsu', new_tozitsu_path %>
